//Class_Array

class Car {
    constructor(name, year) {
      this.name = name;
      this.year = year;
    }
    age() {
      let date = new Date();
      return date.getFullYear() - this.year;
    }
  }
  
  let myCar = new Car("Ford", 2014);

console.log("My car is " + myCar.age() + " years old.");


//Class

class OwnerInfo {
    constructor(owner, BoughtYear) {
      this.name = owner;
      this.year = BoughtYear;
    }
    OldCar() {
      let date = new Date();
      return date.getFullYear() - this.year;
    }
  }
  
  let newCar = new OwnerInfo("Amanda", 2021);

console.log("I have owned my car for " + newCar.OldCar() + " years.");
